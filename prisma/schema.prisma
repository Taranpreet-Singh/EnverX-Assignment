generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model blogPost {
  postId      String   @id @default(uuid()) @map("post_id")
  createdOn   DateTime @default(now()) @map("created_at")
  title       String   @db.VarChar
  description String?  @db.VarChar
}
