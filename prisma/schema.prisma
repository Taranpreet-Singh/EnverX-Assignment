generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model posts {
  post_id     String  @id(map: "posts_pk") @db.Uuid
  created_on  String  @db.VarChar
  post        String  @db.VarChar
  description String? @db.VarChar
}
